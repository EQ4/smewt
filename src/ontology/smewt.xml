<?xml version="1.0"?>

<rdf:RDF 
xmlns:rdf= "http://www.w3.org/1999/02/22-rdf-syntax-ns#" 
xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
xmlns:xesam=" http://freedesktop.org/standards/xesam/1.0/core#"
xml:base=  "http://www.smewt.com/0.1/smewt.rdf#">

<rdfs:Class rdf:ID="Media" />

<rdfs:Class rdf:ID="Metadata" />

<!--
Metadata
-->

<rdfs:Class rdf:ID="Episode">
  <rdfs:subClassOf rdf:resource="#Metadata" />
</rdfs:Class>

<rdfs:Class rdf:ID="Series">
  <rdfs:subClassOf rdf:resource="#Metadata" />
</rdfs:Class>

<rdfs:Class rdf:ID="Poster">
  <rdfs:subClassOf rdf:resource="#Metadata" />
</rdfs:Class>

<rdfs:Class rdf:ID="Subtitle">
  <rdfs:subClassOf rdf:resource="#Metadata" />
</rdfs:Class>

<!--
Media
-->

<rdfs:Class rdf:ID="Video">
  <rdfs:subClassOf rdf:resource="xesam:Video" />
  <rdfs:subClassOf rdf:resource="#Media" />
</rdfs:Class>

<rdfs:Class rdf:ID="Audio">
  <rdfs:subClassOf rdf:resource="xesam:Audio" />
  <rdfs:subClassOf rdf:resource="#Media" />
</rdfs:Class>

<rdfs:Class rdf:ID="Text">
  <rdfs:subClassOf rdf:resource="xesam:Text" />
  <rdfs:subClassOf rdf:resource="#Media" />
</rdfs:Class>

<rdfs:Class rdf:ID="Image">
  <rdfs:subClassOf rdf:resource="xesam:Image" />
  <rdfs:subClassOf rdf:resource="#Media" />
</rdfs:Class>

<!--
Properties
-->

<rdf:Property rdf:ID="hasEpisode">
  <rdfs:domain rdf:resource="#Video" />
  <rdfs:range rdf:resource="#Episode" />
</rdf:Property>

<rdf:Property rdf:ID="hasSubtitle">
  <rdfs:domain rdf:resource="#Video" />
  <rdfs:domain rdf:resource="#Text" />
  <rdfs:range rdf:resource="#Subtitle" />
</rdf:Property>

<rdf:Property rdf:ID="isPoster">
  <rdfs:domain rdf:resource="#Image" />
  <rdfs:range rdf:resource="#Poster" />
</rdf:Property>

<rdf:Property rdf:ID="title">
  <rdfs:domain rdf:resource="#Episode" />
  <rdfs:domain rdf:resource="#Series" />
  <rdfs:range rdf:resource="rdfs:Literal" />
</rdf:Property>

<rdf:Property rdf:ID="episodeNumber">
  <rdfs:domain rdf:resource="#Episode" />
  <rdfs:range rdf:resource="rdfs:Literal" />
</rdf:Property>

<rdf:Property rdf:ID="seasonNumber">
  <rdfs:domain rdf:resource="#Episode" />
  <rdfs:range rdf:resource="rdfs:Literal" />
</rdf:Property>

<rdf:Property rdf:ID="synopsis">
  <rdfs:domain rdf:resource="#Episode" />
  <rdfs:domain rdf:resource="#Series" />
  <rdfs:range rdf:resource="rdfs:Literal" />
</rdf:Property>

<rdf:Property rdf:ID="imdbLink">
  <rdfs:domain rdf:resource="#Episode" />
  <rdfs:domain rdf:resource="#Series" />
  <rdfs:range rdf:resource="rdfs:Literal" />
</rdf:Property>

<rdf:Property rdf:ID="ofSeries">
  <rdfs:domain rdf:resource="#Episode" />
  <rdfs:domain rdf:resource="#Poster" />
  <rdfs:range rdf:resource="#Series" />
</rdf:Property>

<rdf:Property rdf:ID="ofEpisode">
  <rdfs:domain rdf:resource="#Subtitle" />
  <rdfs:range rdf:resource="#Episode" />
</rdf:Property>


</rdf:RDF>
